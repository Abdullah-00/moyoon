<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Sign_up"
        android:background="@drawable/background_color">
    <ImageButton
            android:layout_width="180dp"
            android:layout_height="69dp" app:srcCompat="@drawable/facebooklogin"
            android:id="@+id/FB"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="200dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <ImageButton
            android:layout_width="180dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/googlelogin"
            android:id="@+id/google" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="224dp"
            app:layout_constraintBottom_toTopOf="@+id/FB" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="1.0"/>
</android.support.constraint.ConstraintLayout>